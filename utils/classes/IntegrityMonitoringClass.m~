    properties (SetAccess = immutable)
        M % size of the preceding horizon in epochs
    end
    
    methods
        function obj= IntegrityMonitoringClass(obj, params)
            obj.M= params.preceding_horizon_size;
            
        end
        
        function pred(obj)
        end
    end
end




